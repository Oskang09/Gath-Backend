{
    "compilerOptions": {
        "target": "ES5",
        "module": "commonjs",
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./",
        "paths": {
            "@config/*": [
                "./config/*"
            ],
            "@register/*": [
                "./tasks/*"
            ],
            "@actions/*": [
                "./util/actions/*"
            ],
            "@batch/*": [
                "./util/batch/*"
            ],
            "@hooks": [
                "./util/hook"
            ],
            "@checkers": [
                "./util/boolean"
            ],
            "@getters": [
                "./util/getter"
            ],
            "@utils": [
                "./util/utility"
            ],
        }
    }
}